{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "overrides": [
    { "files": "*.js", "options": { "singleQuote": true } },
    { "files": "*.jsx", "options": { "singleQuote": true, "jsxSingleQuote": true } }
  ],
  "trailingComma": "es5",
  "quoteProps": "preserve",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "lf"
}
